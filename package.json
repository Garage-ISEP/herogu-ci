  "bugs": {
    "url": "https://github.com/Totodore/github-ci/issues"
  },
  "homepage": "https://github.com/Totodore/github-ci#readme",
  "dependencies": {
    "dockerode": "^3.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemailer": "^6.4.17"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.1",
    "@types/express": "^4.17.9",
    "@types/nodemailer": "^6.4.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.2"
  }
